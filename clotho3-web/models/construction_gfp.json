{
    "name" : "Construction File Demo",
    "id" : "ee2f87dec2e6d9c71a57aa11",
    "schema" : "ConstructionFile",
    "description" : "The Construction File automates the construction of pUC19-GFP",
    "dictionary" : {
        "primer1" : "gagctcgaattccctaactaactaaag",
        "primer2" : "ctaattaagcttattaagctac",
        "pPROBE_GFP" : "",
        "EcoRI" : {
            "Clotho" : true,
            "value" : "Digest.enzymes.EcoRI"
        },
        "HindIII" : {
            "Clotho" : true,
            "value" : "Digest.enzymes.HindIII"
        },
        "pUC19" : ""
    },
    "steps" : [
        {
            "reaction" : "PCR.predict",
            "inputs" : ["pPROBE_GFP", ["primer1", "primer2"]],
            "output" : "pcr_product"
        },
        {
            "reaction" : "Digest.digest",
            "inputs" : ["pcr_product", ["EcoRI", "HindIII"]],
            "output" : "fragments"
        },
        {
            "reaction" : "Digest.gelPurify",
            "inputs" : ["fragments", "812"],
            "output" : "probeDigested"
        },
        {
            "reaction" : "Digest.digest",
            "inputs" : ["pUC19", ["EcoRI", "HindIII"], "true"],
            "output" : "fragments2"
        },
        {
            "reaction" : "Digest.gelPurify",
            "inputs" : ["fragments2"],
            "output" : "pUC19Digested"
        },
        {
            "reaction" : "PCR.ligate",
            "inputs" : [["probeDigested", "pUC19Digested"]],
            "output" : "final"
        }
    ]
}